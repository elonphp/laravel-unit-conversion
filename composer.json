{
    "name": "elonphp/laravel-unit-conversion",
    "description": "Laravel unit conversion package with multilingual support and entity-specific conversions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Elon",
            "email": "elonphp@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Elonphp\\UnitConversion\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elonphp\\UnitConversion\\UnitConversionServiceProvider"
            ],
            "aliases": {
                "UnitConverter": "Elonphp\\UnitConversion\\Facades\\UnitConverter"
            }
        }
    },
    "minimum-stability": "stable"
}
